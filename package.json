{
  "name": "cairo-atoms",
  "version": "1.0.0",
  "main": "lib/index.js",
  "module": "lib-esm/index.js",
  "typings": "lib/index.d.ts",
  "repository": "https://github.com/Cairo-design-system/components.git",
  "keywords": [
    "react",
    "components",
    "library",
    "design-system"
  ],
  "files": [
    "codemods",
    "dist",
    "lib",
    "lib-esm",
    "index.d.ts",
    "!lib/__tests__",
    "!lib-esm/__tests__"
  ],
  "author": "Joel Henrique Silva Santos <joelhenrique2000@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "cd docs && yarn run develop",
    "dist": "concurrently yarn:dist:*",
    "dist:rollup": "cross-env NODE_ENV=production rollup -c",
    "dist:transpile:cjs": "cross-env BABEL_MODULE=commonjs babel src --out-dir lib --extensions '.js,.ts,.jsx,.tsx'",
    "dist:transpile:esm": "babel src --out-dir lib-esm --extensions '.js,.ts,.jsx,.tsx'",
    "dist:types": "tsc",
    "lint": "eslint '**/*.{js,ts,tsx}'",
    "lint:fix": "eslint '**/*.{js,ts,tsx}' --fix",
    "test": "jest -- src",
    "update-snapshots": "jest -u -- src",
    "watch": "jest --watch --no-coverage",
    "playroom:start": "playroom start",
    "playroom:build": "playroom build",
    "build": "yarn playroom:build",
    "release": "yarn dist && changeset publish",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
