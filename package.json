    "jscodeshift": "^0.11.0",
    "lodash.isempty": "4.4.0",
    "lodash.isobject": "3.0.2",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^5.0.0",
    "playroom": "^0.23.0",
    "postcss-preset-env": "^6.7.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "rollup": "^2.45.1",
    "rollup-plugin-babel": "^4.4.0",
