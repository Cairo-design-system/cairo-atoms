    "autoprefixer": "^9.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-add-react-displayname": "^0.0.5",
    "babel-plugin-macros": "^3.0.1",
    "babel-plugin-preval": "^5.0.0",
